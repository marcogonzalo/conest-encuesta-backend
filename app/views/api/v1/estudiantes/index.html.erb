<p id="notice"><%= notice %></p>

<h1>Listing Estudiantes</h1>

<table>
  <thead>
    <tr>
      <th>Cedula</th>
      <th>Primer nombre</th>
      <th>Segundo nombre</th>
      <th>Primer apellido</th>
      <th>Segundo apellido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estudiantes.each do |estudiante| %>
      <tr>
        <td><%= estudiante.cedula %></td>
        <td><%= estudiante.primer_nombre %></td>
        <td><%= estudiante.segundo_nombre %></td>
        <td><%= estudiante.primer_apellido %></td>
        <td><%= estudiante.segundo_apellido %></td>
        <td><%= link_to 'Show', estudiante %></td>
        <td><%= link_to 'Edit', edit_estudiante_path(estudiante) %></td>
        <td><%= link_to 'Destroy', estudiante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estudiante', new_estudiante_path %>
