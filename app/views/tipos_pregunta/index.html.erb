<p id="notice"><%= notice %></p>

<h1>Listing Tipos Pregunta</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Valor</th>
      <th>Valor html</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos_pregunta.each do |tipo_pregunta| %>
      <tr>
        <td><%= tipo_pregunta.nombre %></td>
        <td><%= tipo_pregunta.valor %></td>
        <td><%= tipo_pregunta.valor_html %></td>
        <td><%= link_to 'Show', tipo_pregunta %></td>
        <td><%= link_to 'Edit', edit_tipo_pregunta_path(tipo_pregunta) %></td>
        <td><%= link_to 'Destroy', tipo_pregunta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo pregunta', new_tipo_pregunta_path %>
